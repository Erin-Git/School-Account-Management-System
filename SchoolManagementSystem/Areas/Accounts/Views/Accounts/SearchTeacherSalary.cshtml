@model SchoolManagementSystem.Areas.Accounts.ViewModels.FeeSubViewModel









<form style="margin-top:20px;padding:20px;background-color:ghostwhite; box-shadow:0 5px 15px #686868;border-radius:10px 15px;" asp-area="Accounts" asp-controller="Accounts" asp-action="SearchResult" method="post">
    <h1 style="text-align:center;font-family:'Century Gothic';font-weight:bold;">Search Teacher Salary</h1>
    <hr style="width:70%;" />

    <div class="row" style="padding:5px;">
        @*row 1*@
        <div class="col-xs-12 col-sm-12 col-md-4 col-md-offset-2 col-lg-4 col-lg-offset-2">
            <div class="form-group">
                <label>Enter:</label>

                <input asp-for="dateVM" id="date" class="form-control" type="date" />
                <input />
                <span class="text-danger field-validation-valid" asp-validation-for="dateVM"></span>
                @*@ViewBag.msg1*@
            </div>
        </div>
      
    </div>
    <div class="row" style="padding:10px;">
        @*row 2*@
        <div class="col-xs-12 col-sm-12 col-md-2 col-lg-2" style="float:right;">
            <button class="btn btn-primary" type="submit" style="float:right;">Submit</button>
        </div>
    </div>
</form>

